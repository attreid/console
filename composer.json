{
  "name": "attreid/console",
  "description": "Nette Cli console with web interface",
  "type": "library",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Attreid"
    }
  ],
  "require": {
    "php": ">= 8.4",
    "nette/application": "^3.1",
    "nette/di": "^3.0",
    "tracy/tracy": "~2.4"
  },
  "require-dev": {
    "nette/tester": "~2.4"
  },
  "scripts": {
    "test": "tester tests",
    "test:coverage": "tester -c tests/php.ini --coverage ./coverage.html --coverage-src ./src tests"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}